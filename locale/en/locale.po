msgid "plugins.importexport.zenodo.displayName"
msgstr "Zenodo Export Plugin"

msgid "plugins.importexport.zenodo.description"
msgstr "Export journal articles to Zenodo."

msgid "plugins.importexport.zenodo.export.createAccount"
msgstr "Create a Zenodo account."

msgid "plugins.importexport.zenodo.registrationIntro"
msgstr ""
"If you would like to register articles from within OJS, please enter your "
"Zenodo API Key. You will find your API key on your Zenodo account's \"Applications\" page."

msgid "plugins.importexport.zenodo.settings.form.apiKey"
msgstr "Zenodo API Key"

msgid "plugins.importexport.zenodo.settings.form.community"
msgstr "Zenodo Community"

msgid "plugins.importexport.zenodo.settings.form.community.description"
msgstr ""
"The Zenodo community to submit records to. Please enter the community's unique identifier; "
" for example, to submit to the community <a href=\"https://zenodo.org/community/eu\">https://zenodo.org/community/eu</a>, enter 'eu'."

msgid "plugins.importexport.zenodo.settings.form.automaticPublishingCommunity.description"
msgstr ""
"<strong>Accept Community Reviews</strong>: Automatically accept review requests to this Zenodo community. "
"This will only apply to communities which require submissions to be reviewed. "
"Only enable this setting if you have permissions in Zenodo to publish records in this community. This will also publish the record, "
"and can not be deleted once published."

msgid "plugins.importexport.zenodo.settings.form.automaticRegistration.description"
msgstr ""
"<strong>Automatic Deposit</strong>: Deposit articles automatically to Zenodo. Please note that this may "
"take a short amount of time after publication to process (e.g. depending on "
"your cronjob configuration)."

msgid "plugins.importexport.zenodo.settings.form.automaticPublishing.description"
msgstr ""
"<strong>Automatic Publishing</strong>: Publish records automatically in Zenodo. If not enabled, records "
"will be created as drafts and can be published in the Zenodo platform. Records can not be deleted in Zenodo "
" once they are published."

msgid "plugins.importexport.zenodo.settings.form.doi.description"
msgstr ""
"<strong>Zenodo DOIs:</strong> Allow Zenodo to mint DOIs when a DOI has not been created in OJS. If not enabled, objects will not be deposited"
" unless they have a DOI."

msgid "plugins.importexport.zenodo.settings.form.testMode.description"
msgstr ""
"<strong>Test Mode:</strong> Use the <a href=\"https://sandbox.zenodo.org\">Zenodo Sandbox</a> API "
"for registration. "
"Please do not forget to disable this option for production."

msgid "plugins.importexport.zenodo.senderTask.name"
msgstr "Zenodo automatic registration task"

msgid "plugins.importexport.zenodo.register.error.noApiKey"
msgstr "No Zenodo API Key was found. Please set the API key and try again."

msgid "plugins.importexport.zenodo.register.error.mdsError"
msgstr "Deposit was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.register.error.communityError"
msgstr "Unable to retrieve the community ID from Zenodo. Please check the community is set correctly and try again."

msgid "plugins.importexport.zenodo.api.error.communityIdError"
msgstr "Retrieving the community ID was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.api.error.fileError"
msgstr "File deposit was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.api.error.fileDeleteError"
msgstr "Deleting the file in Zenodo was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.api.error.recordDeleteError"
msgstr "Deleting the draft record in Zenodo was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.api.error.awardError"
msgstr "The award validation check was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.api.error.publishError"
msgstr "Publishing was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.api.error.publishCheckError"
msgstr "The record publish check was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.api.error.createReviewError"
msgstr "Creating the review request was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.api.error.submitReviewError"
msgstr "Submitting the review request was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.api.error.acceptReviewError"
msgstr "Accepting the review request was not successful! The Zenodo API returned an error: '{$param}'."

msgid "plugins.importexport.zenodo.api.error.noDoi"
msgstr "An object you have selected does not have a DOI assigned. Please assign a DOI and try again."

msgid "plugins.importexport.zenodo.cliUsage"
msgstr ""
"Usage:\n"
"{$scriptName} {$pluginName} export [xmlFileName] [journal_path] articles "
"objectId1 [objectId2] ...\n"
